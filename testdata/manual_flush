batch
set a 1
set b 2
----

# The first L0 table can have its seqnums zeroed.
flush
----
0:
  5:[a#0,SET-b#1,SET]

reset
----

batch
set a 1
set b 2
del a
del b
----

flush
----
0:
  5:[a#2,DEL-b#3,DEL]

batch
set a 3
----

# A second (overlapping) L0 table will have non-zero seqnums.
flush
----
0:
  5:[a#2,DEL-b#3,DEL]
  7:[a#4,SET-a#4,SET]

batch
set c 4
----

# A third (non-overlapping) L0 table will have non-zero seqnums.
flush
----
0:
  5:[a#2,DEL-b#3,DEL]
  7:[a#4,SET-a#4,SET]
  9:[c#5,SET-c#5,SET]

reset
----

batch
set a 1
set b 2
del-range a c
----

flush
----

reset
----

batch
set a 1
set b 2
----

async-flush
----
0:
  5:[a#0,SET-b#1,SET]
